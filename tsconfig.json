{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/admin/edit/page.tsx", "src/app/home/page.tsx", "src/app/components/product.tsx", "src/app/user/register/page.jsx", "src/app/user/profile/information/page.jsx", "src/app/cart/[id]/page.tsx", "src/app/user/profile/changepassword/page.jsx", "src/app/api/auth/signup/route.js", "src/app/checkout/page.tsx", "src/app/bill/page.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
